{
    "name": "jvancoillie/ldap-filter-lexer",
    "description": "A library validating ldap filters",
    "type": "library",
    "authors": [
        {"name": "Jérémy Vancoillie"}
    ],
    "extra": {
        "branch-alias": {
            "dev-main": "0.0.x-dev"
        }
    },
    "require": {
        "php": ">=8.1",
        "doctrine/lexer": "^2.0 || ^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.2",
        "vimeo/psalm": "^5.12",
        "friendsofphp/php-cs-fixer": "^3.64"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Jvancoillie\\LdapFilterLexer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jvancoillie\\LdapFilterLexer\\Tests\\": "tests"
        }
    }
}
